<view class="section">
  <picker mode="date" value="{{date}}" bindchange="bindDateChange">
    <view class="picker">
        <button type="default" size="{{default}}" style="font-size:26rpx;">{{date !==null ? '您的生日：'+date :'点击设置您的生日'}}</button>
    </view>
  </picker>
</view>